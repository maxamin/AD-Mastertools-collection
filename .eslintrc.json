{
    "env": {
        "browser": true,
        "node":true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended",
        "prettier",
        "plugin:react-hooks/recommended"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
    },
    "globals": {
        "emitter": "writable",
        "appStore": "writable",
        "closeTooltip": "readonly",
        "$": "readonly",
        "sigma": "readonly",
        "driver": "readonly",
        "conf": "readonly",
        "jQuery": "readonly"
    }
}
